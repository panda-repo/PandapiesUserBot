{
      "name": "PandapiesUserBot",
      "description": "An Anime themed Telegram userbot.",
      "logo": "https://telegra.ph/file/bf18fb39dad5fb3ab9e7e.png",
      "keywords": [
         "telegram",
         "anime",
         "advanced",
         "userbot",
         "PandapiesRobot"
         "PandapiesUserBot"
      ],   
   "repository": "https://github.com/panda-repo/PandapiesUserBot",
   "addons": [
      {
         "options": {
            "version": "12"
         },
         "plan": "heroku-postgresql"
      }
   ],
   "env": {
      "BOT_TOKEN": {
         "description": "Bot Token Kamu,Ambil Di @BotFather duh",
         "required": true,
         "value": ""
      },
      "API_ID": {
         "description": "Ambil API_ID di my.telegram.org, used for telethon based modules.",
         "required": true,
         "value": ""
      },
      "API_HASH": {
         "description": "Get API_HASH from my.telegram.org, used for telethon based modules.",
         "required": true,
         "value": ""
      },    
      "SESSION": {
         "description": "Your Pyrogram String Session.",
         "required": true,
         "value": ""
     }
         },
        "LOG_GROUP_ID": {
      "description": "Log Grup Kamu.",
         "required": true,
         "value": "-100"
        }
          },
          "OWNER_ID": {
      "description": "ID Tele Kamu.",
         "required": true,
         "value": ""
          },
      }
